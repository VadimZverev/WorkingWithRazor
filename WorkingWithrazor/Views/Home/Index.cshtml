@model string[]

@{
    ViewBag.Title = "Index";
}

@section Header{
    <div class="view">
        @foreach (string str in new[] { "Home", "List", "Edit" })
        {
            @Html.ActionLink(str, str, null, new { style = "margin: 5px" })
        }
    </div>
}

@section Body{
    <div class="view">
        Это список названия продуктов:
        @foreach (string name in Model)
        {
            <span><b>@name</b></span>
        }
    </div>
}

@section Footer{
    <div class="view">
        Это footer(подвал)
    </div>
}